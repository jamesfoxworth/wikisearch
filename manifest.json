{
  "manifest_version": 3,
  "name": "Wikisearch",
  "version": "1.0",
  "description": "Quickly search for something on Wikipedia in a new tab.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon-16.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }

  },
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ]
    }
